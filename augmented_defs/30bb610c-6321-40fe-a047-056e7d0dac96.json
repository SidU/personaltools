      "botId": "4aa38041-66a2-41a4-ac97-55bc828a9803",
      "canUpdateConfiguration": false,
      "commands": [
        {
          "description": "Search confluence pages by title or content keywords",
          "fetchTask": false,
          "id": "confluenceSearch",
          "initialRun": true,
          "parameters": [
            {
              "description": "Title or content keywords",
              "inputType": "text",
              "isRequired": false,
              "name": "searchKeyword",
              "title": "Keywords"
            },
            {
              "description": "A precise Confluence query language generated from the user prompt. This is a mandatory parameter following standard formatting.",
              "inputType": "text",
              "isRequired": false,
              "name": "cql",
              "title": "CQL"
            },
            {
              "description": "'To find pages which I updated/watch/was mentioned' use: 'contributor = currentUser() OR watcher/mention = currentUser()'",
              "inputType": "text",
              "isRequired": false,
              "name": "cqlIns1",
              "title": "CQL Example Translation 1"
            },
            {
              "description": "To filter by time or label(like 'design') use the syntax: created > 'YYYY-MM-DD', lastmodified < 'YYYY-MM-DD', label = \"design\"",
              "inputType": "text",
              "isRequired": false,
              "name": "cqlIns2",
              "title": "CQL Example Translation 2"
            },
            {
              "description": "Find pages with comments made by me containing the phrase design= commenter = currentUser() AND text ~ \"design\"",
              "inputType": "text",
              "isRequired": false,
              "name": "cqlIns3",
              "title": "CQL Example Translation 3"
            }
          ],
          "samplePrompts": [
            {
              "text": "Find pages that I have been mentioned on recently"
            },
            {
              "text": "Find pages edited by me"
            },
            {
              "text": "Find pages that I watched in the last week"
            }
          ],
