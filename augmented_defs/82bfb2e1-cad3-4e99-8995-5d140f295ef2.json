      "botId": "82bfb2e1-cad3-4e99-8995-5d140f295ef2",
      "canUpdateConfiguration": false,
      "commands": [
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "See the most recent checkin responses",
          "fetchTask": true,
          "id": "checkinResponses",
          "initialRun": true,
          "parameters": [
            {
              "description": "The name of the check-in to see responses (Optional)",
              "inputType": "text",
              "isRequired": false,
              "name": "followupsName",
              "title": "Check-in name"
            },
            {
              "description": "The name of the persons to see responses (Example format json stringify: '[\"userName1\",\"userName2\"]')) (Optional)",
              "inputType": "text",
              "isRequired": false,
              "name": "personName",
              "title": "Name of the persons"
            },
            {
              "description": "The start date to filter responses (Format: MM/dd/yyyy) (Optional) (Mandatory if endDate is set)",
              "inputType": "date",
              "isRequired": false,
              "name": "startDate",
              "title": "Start date"
            },
            {
              "description": "The end date to filter responses (Format: MM/dd/yyyy) (Optional) (Mandatory if startDate is set)",
              "inputType": "date",
              "isRequired": false,
              "name": "endDate",
              "title": "End date"
            }
          ],
          "samplePrompts": [
            {
              "text": "Could you provide my check-in responses from today?"
            },
            {
              "text": "Please share the check-in responses from {User1} and {User1}."
            },
            {
              "text": "Could you get the check-in responses from the daily standup check-in of last week?"
            },
            {
              "text": "I'd like to see the check-in responses from last month."
            }
          ],
          "semanticDescription": "This command retrieves the most recent responses from specified check-ins within DailyBot Organization. Users can specify a check-in by name to filter responses related to that particular session. Additionally, responses can be filtered by the names of participants using a JSON array format. Date filters can also be applied to narrow down the responses between specific start and end dates. The command supports optional parameters for check-in names, participant names, and dates, providing flexibility in querying data. The date format required is MM/dd/yyyy, and if an end date is provided, a start date must also be specified to define the range of the query.",
          "title": "Get check-in responses",
          "type": "query"
        },
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Quick summary of a person's activity in DailyBot",
          "fetchTask": true,
          "id": "searchPeople",
          "initialRun": true,
          "parameters": [
            {
              "description": "Search DailyBot User for individual reports. Output: String.",
              "inputType": "text",
              "isRequired": false,
              "name": "name",
              "title": "Person name"
            }
          ],
          "samplePrompts": [
            {
              "text": "Give me my report summary of my activity in DailyBot."
            },
            {
              "text": "Using DailyBot, tell me who {User} is."
            },
            {
              "text": "Using DailyBot, Tell me about {User}"
            },
            {
              "text": "Get the daily report of {User}."
            }
          ],
          "semanticDescription": "This command provides a summary of a specified person's within DailyBot Organization. Users can input the name of a person to retrieve a quick report of their activity in DailyBot. The command is designed to fetch data related to individual users, summarizing their participation and responses in various check-ins and more activities in DailyBot. The parameter 'name' accepts a text input representing the person's name, and the command returns a summary of the user. This command is useful for managers or team leaders who need to quickly assess team member engagement and activity.",
          "title": "See Individual report",
          "type": "query"
        },
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Interactively ask DailyBot questions about your team and progress, project details, or any general inquiries.",
          "fetchTask": true,
          "id": "askDailyBot",
          "initialRun": false,
          "parameters": [
            {
              "description": "Type your question for DailyBot. You can ask about team progress, project details, or any general inquiries.",
              "inputType": "text",
              "isRequired": false,
              "name": "question",
              "title": "Your Question"
            }
          ],
          "samplePrompts": [
            {
              "text": "Using DailyBot, tell me what DailyBot does."
            },
            {
              "text": "Ask to DailyBot about How can I configure time off periods in DailyBot?"
            },
            {
              "text": "Ask to DailyBot, How can I get support or help from DailyBot regarding its features?"
            },
            {
              "text": "Ask to DailyBot, what is the way to give kudos in DailyBot?"
            }
          ],
          "semanticDescription": "This command allows users to interactively query DailyBot about various aspects of their team and project management. Users can ask questions related to team progress, project details, or general inquiries about DailyBot's functionalities. The command is designed to facilitate on-the-fly information retrieval and decision-making. It accepts a single parameter, 'question', where users can input their specific queries. The command processes these inquiries and returns relevant information or guidance based on DailyBot's integrated knowledge and capabilities. This feature is particularly useful for team leaders and project managers seeking quick insights or needing to address specific concerns about their projects or team dynamics.",
          "title": "Ask DailyBot for AI Insights",
          "type": "query"
        }
      ],
      "fetchCommands": false,
      "scopes": [
        "team",
        "personal",
        "groupChat"
      ]
    }
