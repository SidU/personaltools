      "commandLists": [
        {
          "commands": [
            {
              "description": "Acknowledge the alerts with IDs",
              "title": "ack [tiny-id]"
            },
            {
              "description": "Snooze the alerts with IDs for the specified time",
              "title": "snooze [tiny-id] for m/h/d"
            },
            {
              "description": "Add note to the alerts with IDs",
              "title": "note [note] to [tiny-id]"
            },
            {
              "description": "Acknowledge/Close all open alerts",
              "title": "ackall/closeall"
            },
            {
              "description": "Close the alerts with IDs",
              "title": "close [tiny-id]"
            },
            {
              "description": "Execute the [action] on the alerts with IDs",
              "title": "exec [action-name] on [tiny-id]"
            },
            {
              "description": "Get details of the alert with ID",
              "title": "get [tiny-id]"
            },
            {
              "description": "Create an alert with the message for the specified recipients/teams",
              "title": "alert [msg] for [team1,user1...]"
            },
            {
              "description": "Lists available commands for Opsgenie",
              "title": "help"
            },
            {
              "description": "Connect your Opsgenie account to your Microsoft Teams account",
              "title": "connect"
            }
          ],
          "scopes": [
            "team"
          ]
