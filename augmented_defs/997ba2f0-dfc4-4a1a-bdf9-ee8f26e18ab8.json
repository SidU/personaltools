      "botId": "ebc9fd31-0a5e-4686-b7d9-70d5f7a59fa7",
      "canUpdateConfiguration": false,
      "commands": [
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Ask Ramp any question",
          "fetchTask": false,
          "id": "RampQuery",
          "initialRun": false,
          "parameters": [
            {
              "description": "A question about your company expense policy",
              "inputType": "text",
              "isRequired": false,
              "name": "expense_policy_query",
              "semanticDescription": "A natural language question about a company expense policy. Example input: 'Do I need a receipt if dinner costs less than $50?' Output: A natural language response, e.g. 'No, your expense policy only requires a receipt for expenses over $75.'",
              "title": "Expense Policy Query"
            },
            {
              "description": "A question about your spending",
              "inputType": "text",
              "isRequired": false,
              "name": "spend_query",
              "semanticDescription": "A natural language question about the company's spending. Ramp has data on the company's people, transactions, reporting chains, spending categories, and more, and can leverage this data to answer a wide range of user questions. Example input: 'How much has the marketing team spent on hotels in the past year?' Output: A tabular representation of the data being requested.",
              "title": "Spend Query"
            },
            {
              "description": "A question about finance team's organization",
              "inputType": "text",
              "isRequired": false,
              "name": "finance_team_query",
              "semanticDescription": "A natural language question about the company's org structure within Ramp. Example input: 'Who are the finance administrators in my company?' Output: A list of the finance administrators in the company.",
              "title": "Finance Team Query"
            }
          ],
          "samplePrompts": [
            {
              "text": "Ramp do I need a receipt if dinner costs less than $50?"
            },
            {
              "text": "Ramp what airlines did our team spend the most last year?"
            },
            {
              "text": "Ramp what SaaS vendors have we paid the most last year?"
            }
          ],
          "semanticDescription": "Ask Ramp any question. Ramp is a spend management platform and can answer a wide range of questions about your company's spending, expense policies, organizational structure, and more.",
          "title": "Ramp Query",
          "type": "query"
        }
      ],
      "fetchCommands": false,
      "scopes": [
        "team",
        "personal",
        "groupChat"
      ]
    }
  ],
