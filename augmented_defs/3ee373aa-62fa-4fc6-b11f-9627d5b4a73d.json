  "inputExtensions": [
    {
      "botId": "e6a504a1-9e11-469c-bcd4-69a52470230e",
      "canUpdateConfiguration": true,
      "commands": [
        {
          "description": "Search for Matters",
          "fetchTask": false,
          "id": "searchMatters",
          "initialRun": true,
          "parameters": [
            {
              "description": "Enter keywords to search LawToolbox Matter or deadlines; if querying specific user's matter or deadlines, leave input empty",
              "inputType": "text",
              "isRequired": false,
              "name": "SearchMatter",
              "semanticDescription": "The 'SearchMatter' parameter enables searching for matters or deadlines within LawToolBox using specific keywords or descriptive terms provided by the user. Valid matter names or descriptive terms should be used to direct the search. If a user inputs 'my matters', treat this as a command to retrieve all matters created by the user. In such cases, set the SearchMatter parameter to empty and filter the results by the user's creator ID. Similarly, if a user inputs 'recent matters', treat this as a command to retrieve all recently created matters. This method ensures accurate interpretation of user intent and provides relevant search results without misinterpreting these phrases as specific keywords.",
              "title": "MatterSearch"
            },
            {
              "description": "Retrieve email(s) of mentioned user(s) for 'associated' matter in LawToolBox. Exclude requester's email.",
              "inputType": "text",
              "isRequired": false,
              "name": "useremail",
              "semanticDescription": "The useremail parameter is used to specify the email address(es) of user(s) associated with a matter in LawToolbox. Provide up to 2 valid email addresses separated by commas, excluding the requester's own email. Use this parameter to filter matters based on the email address(es) of specific users, ensuring that only matters associated with those users are retrieved.",
              "title": "user email"
            },
            {
              "description": "Set 'isMyMatter' to true if there's mention of 'my', 'me', 'myself', 'my matters', 'my matter' or related terms in the context.",
              "inputType": "toggle",
              "isRequired": false,
              "name": "ismymatter",
              "semanticDescription": "Set isMyMatter to true if there is a mention of 'my', 'me', 'myself', 'my matters', 'my matter' or any similar pronouns or adjectives that refer to the speaker/writer in the context. Otherwise, set isMyMatter to false.",
              "title": "Is my Matter"
            },
            {
              "description": "date 'MM/dd/yyyy' or daterange like 'MM/dd/yyyy-MM/dd/yyyy' if available else leave empty.",
              "inputType": "text",
              "isRequired": false,
              "name": "daterange",
              "semanticDescription": "The `daterange` parameter is designed to interpret and process dates or date ranges from phrases. It accepts input in following formats: A single date (e.g., 'MM/dd/yyyy'), A date range (e.g., 'MM/dd/yyyy - MM/dd/yyyy') or it could be empty/blank if dates are not available in phrases. And for the contextual time frame such as: 'this week' - add date range of 7 days of current week, for example if this week start from 06/13/2024 and end on 06/19/2024 but the current date is 06/15/2024 then add date range of 7 days (06/13/2024 to 06/19/2024). 'next week' - add date range of 7 days of next week and start date must be date after current week, for example current date is 06/15/2024 and this week end on 06/19/202 then add date range of 7 days (06/20/2024 to 06/26/2024) after this week end. 'week' - add date range of 7 days from current date.",
              "title": "dateRage"
            },
            {
              "description": "true if user is asking for deadlines from LawToolBox else false",
              "inputType": "toggle",
              "isRequired": false,
              "name": "fetchdeadlines",
              "semanticDescription": "Set this parameter to true if the user is requesting deadlines, appointments, meetings from LawToolbox. This parameter is boolean and should reflect the user's intent to retrieve deadline data.",
              "title": "deadline"
            }
          ],
          "samplePrompts": [
            {
              "text": "Show my upcoming deadlines in LawToolbox"
            },
            {
              "text": "Show my matters in LawToolbox"
            },
            {
              "text": "Show my deadlines for this week in LawToolbox"
            }
          ],
          "semanticDescription": "Facilitates searching for legal matters based on keywords, user email, and ownership filters",
          "title": "SearchMatters",
          "type": "query"
        },
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Create a new matter",
          "fetchTask": true,
          "id": "creatematter",
          "initialRun": false,
          "parameters": [
            {
              "description": "Name of the matter",
              "inputType": "text",
              "isRequired": false,
              "name": "matterName",
              "semanticDescription": "The matterName parameter accepts a string representing the name of the legal matter to be created. The name should be descriptive, unique, and must be short as much as possible. It may include letters, numbers, spaces.",
              "title": "MatterName"
            }
          ],
          "semanticDescription": "Enables users to create a new legal matter by providing the matter name",
          "title": "Create a matter",
          "type": "action"
        },
        {
          "context": [
            "message"
          ],
          "description": "Find deadline from the message",
          "fetchTask": true,
          "id": "finddeadlines",
          "initialRun": false,
          "parameters": [
            {
              "description": "Keywords or phrases that can help in finding the deadlines",
              "inputType": "text",
              "isRequired": false,
              "name": "param",
              "semanticDescription": "The param parameter is used to pass relevant data required to identify and highlight deadlines within a message. The input should be a string containing keywords or phrases that can help in finding the deadlines. Expected values are text strings relevant to the context of the message.",
              "title": "param"
            }
          ],
          "semanticDescription": "Allows users to find and highlight deadlines mentioned within a message",
          "title": "LawToolBox",
          "type": "action"
        },
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Create an appointment in the matter",
          "fetchTask": true,
          "id": "createappointment",
          "initialRun": false,
          "parameters": [
            {
              "description": "start date of appointment in yyyy/mm/dd",
              "inputType": "text",
              "isRequired": false,
              "name": "startDate",
              "semanticDescription": "The startDate parameter accepts the start date of the appointment in the format 'yyyy/mm/dd'. Ensure the date is accurate and within a reasonable range and appropriately represent the scheduled start of the appointment.",
              "title": "StartDate"
            },
            {
              "description": "end date of appointment in yyyy/mm/dd",
              "inputType": "text",
              "isRequired": false,
              "name": "endDate",
              "semanticDescription": "The endDate parameter accepts the end date of the appointment in the format 'yyyy/mm/dd'. Ensure the date is accurate and within a reasonable range and appropriately represent the scheduled end of the appointment.",
              "title": "EndDate"
            },
            {
              "description": "start time of appointment in HH:mm",
              "inputType": "text",
              "isRequired": false,
              "name": "starttime",
              "semanticDescription": "The starttime parameter specifies the start time of the appointment in 'HH' format, using a 24-hour clock. Ensure the time is accurate and represents the beginning of the appointment.",
              "title": "StartTime"
            },
            {
              "description": "end time of appointment in HH:mm",
              "inputType": "text",
              "isRequired": false,
              "name": "endtime",
              "semanticDescription": "The endtime parameter specifies the end time of the appointment in 'HH' format, using a 24-hour clock. Ensure the time follows the start time and accurately represents the end of the appointment.",
              "title": "EndTime"
            },
            {
              "description": "The description or subject of the appointment",
              "inputType": "textarea",
              "isRequired": false,
              "name": "description",
              "semanticDescription": "Provide a brief description or subject for the appointment. The input should clearly outline the purpose or main topic of the appointment, using up to 300 characters.",
              "title": "Description"
            }
          ],
          "semanticDescription": "Allows users to schedule an appointment within a matter by providing the start and end dates, times, and a description",
          "title": "Create an appointment",
          "type": "action"
        },
        {
          "context": [
            "commandBox",
            "compose"
          ],
          "description": "Get upcoming deadlines for a week",
          "fetchTask": true,
          "id": "myupcomingdeadlines",
          "initialRun": false,
          "parameters": [
            {
              "description": "start date in MM/dd/yyyy format",
              "inputType": "text",
              "isRequired": false,
              "name": "startDate",
              "semanticDescription": "Use this parameter to specify the start date for upcoming deadlines in 'MM/dd/yyyy' format. The date should be within the current or future time frame.",
              "title": "StartDate"
            },
            {
              "description": "end date in MM/dd/yyyy format",
              "inputType": "text",
              "isRequired": false,
              "name": "endDate",
              "semanticDescription": "Use this parameter to specify the end date for upcoming deadlines in 'MM/dd/yyyy' format. The date should be within the current or future time frame.",
              "title": "EndDate"
            }
          ],
          "semanticDescription": "Displays upcoming deadlines within a week by entering the start and end dates",
          "title": "My upcoming deadlines",
          "type": "action"
        }
      ],
