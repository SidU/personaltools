      "botId": "e4218775-2e3c-44eb-807f-78f8f39a04d8",
      "commands": [
        {
          "context": [
            "compose"
          ],
          "description": "Search your Creately workspaces",
          "fetchTask": false,
          "id": "searchCmd",
          "initialRun": true,
          "parameters": [
            {
              "description": "Name of Creately workspace",
              "inputType": "text",
              "isRequired": false,
              "name": "workspaceName",
              "title": "Workspace Name"
            },
            {
              "description": "One or more tag names of a workspace",
              "inputType": "text",
              "isRequired": false,
              "name": "workspaceTags",
              "semanticDescription": "Workspace can have one or more user-defined tags/labels. Output must be comma(,) separated string of tags. Output: 'work,Q1,tags'",
              "title": "Workspace Tags"
            },
            {
              "description": "Status of the workspace",
              "inputType": "text",
              "isRequired": false,
              "name": "workspaceStatus",
              "semanticDescription": "Workspace can have particular status at any given time. Must be one of 'None', 'Draft', 'In Progress', 'In Review', 'Appoved', 'Final' and 'Archived'. Output must be comma(,) separated string of statuses. Output: 'Draft, In Progress'",
              "title": "Workspace Status"
            }
          ],
          "samplePrompts": [
            {
              "text": "Show recent workspaces in Creately"
            },
            {
              "text": "Search Creately workspaces I worked yesterday that are in 'Draft' or 'In Progress'"
            },
            {
              "text": "Which worksapces did I create in last month on Creately?"
            }
          ],
          "semanticDescription": "Search any Creately workspace user has access",
          "title": "Find Workspaces",
          "type": "query"
        },
        {
          "context": [
